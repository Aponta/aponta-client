trol/ToastControl\";\r\nimport { Link, useHistory } from \"react-router-dom\";\r\n\r\nfunction Logar() : JSX.Element {\r\n\r\n  const historico = useHistory();\r\n  const [dados, setDados] = useState({\r\n    usuario: \"\",\r\n    senha: \"\"\r\n  })\r\n\r\n  useEffect(() => {\r\n    if(localStorage.getItem(\"tokenAutenticacao\")){\r\n      historico.push(\"/home\");\r\n    }\r\n  }, [])\r\n\r\n  const handleInputChange = (event : React.ChangeEvent<HTMLInputElement>) => {\r\n    setDados({\r\n      ...dados,\r\n      [event.currentTarget.name]: event.currentTarget.value,\r\n    });\r\n  };\r\n\r\n  const logar = (usuario:string, senha: string ) : void => {\r\n\r\n    const requisicao = {\r\n      usuario: usuario,\r\n      senha: senha\r\n    }\r\n\r\n    backEndUtils.chamarBackEnd(\"POST\", \"/login/logar\", requisicao).then((resposta)=>{\r\n      if(resposta.status == 200){\r\n        resposta.json().then((data)=>{\r\n          if(data.message){\r\n            showToast(\"erro\", data.message);\r\n          }else{\r\n            localStorage.tokenAutenticacao = btoa(data.token);\r\n            localStorage.usuarioLogado = btoa(data.usuario.ID_USUARIO);\r\n            showToast(\"sucesso\", \"login efetuado com sucesso\");\r\n            historico.push(\"/home\")\r\n          }\r\n        })\r\n      }\r\n    })\r\n  }\r\n\r\n    return (\r\n      <>\r\n        <div id=\"container-login\">\r\n          <div id=\"login-titulo\">\r\n            <p className=\"titulo-aponta\">\r\n              APONTA\r\n            </p>\r\n          </div>\r\n          <div id=\"login-corpo\">\r\n            <div className=\"container-corpo-login\">\r\n              <div className=\"form-row pb-2\">\r\n                <div className=\"col\">\r\n                  <input \r\n                  type=\"text\" \r\n                  name=\"usuario\"\r\n                  placeholder=\"Usuário\" \r\n                  className=\"form-control text-center\" \r\n                  onChange={(event) => handleInputChange(event)} />\r\n                </div>\r\n              </div>\r\n              <div className=\"form-row pb-2\">\r\n                <div className=\"col\">\r\n                <input \r\n                type=\"password\" \r\n                name=\"senha\"\r\n                placeholder=\"Senha\" \r\n                className=\"form-control text-center\" \r\n                onChange={(event) => handleInputChange(event)} />\r\n                </div>\r\n              </div>\r\n              <div className=\"form-row pb-4\">\r\n                <div className=\"col\">\r\n                <button \r\n                type=\"button\" \r\n                className=\"btn btn-aponta btn-azul w-100\"\r\n                onClick={()=>logar(dados.usuario, dados.senha)}>\r\n                    Login\r\n                  </button>\r\n                </div>\r\n              </div>\r\n              <div className=\"form-row\">\r\n                <div className=\"col\">\r\n                  <p>Não é cadastrado? \r\n                    <Link to=\"/novo-login\">\r\n                      <a href=\"#\"> Cadastre-se</a>\r\n                    </Link>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </>\r\n    )\r\n}\r\n\r\nconst mapStateToProps = (state : any) => ({});\r\n\r\nconst mapDispatchToProps = (dispatch : any) => ({\r\n  SetUsuarioLogado: (usuarioLogado : any) => \r\n    dispatch(loginActions.SetUsuarioLogado(usuarioLogado))\r\n});\r\n\r\nexport default connect(\r\n  mapStateToProps,\r\n  mapDispatchToProps\r\n)(Logar);","C:\\Users\\marcelo.nogueira\\OneDrive - A. Telecom Teleinformática LTDA\\Documents\\aponta-client\\src\\componentes\\Login\\CadastrarLogin\\CadastrarLogin.tsx",[],"C:\\Users\\marcelo.nogueira\\OneDrive - A. Telecom Teleinformática LTDA\\Documents\\aponta-client\\src\\componentes\\ToastControl\\ToastControl.tsx",[],[],"C:\\Users\\marcelo.nogueira\\OneDrive - A. Telecom Teleinformática LTDA\\Documents\\aponta-client\\src\\utils\\BackEnd.ts",["131","132"],"export const linkBackEnd = process.env.REACT_APP_API_URL;\r\n\r\nexport const chamarBackEnd = async (\r\n    metodo : string, \r\n    caminho : string, \r\n    corpo : any) : Promise<Response> => {\r\n        \r\n    let tokenAutenticacao = localStorage.getItem(\"tokenAutenticacao\")\r\n\r\n    if(tokenAutenticacao){\r\n        tokenAutenticacao = atob(tokenAutenticacao)\r\n    }\r\n\r\n    if(corpo){\r\n        return await fetch(linkBackEnd + caminho, {\r\n            method: metodo,\r\n            headers: { \r\n                \"Content-Type\": \"application/json\",\r\n                \"Authorization\": \"Bearer \" + tokenAutenticacao\r\n                },\r\n            body: JSON.stringify(corpo),\r\n            }).then((resposta) =>  {\r\n            return resposta\r\n        })\r\n    }else{\r\n        return await fetch(linkBackEnd + caminho, {\r\n            method: metodo,\r\n            headers: { \r\n                \"Content-Type\": \"application/json\",\r\n                \"Authorization\": \"Bearer \" + tokenAutenticacao\r\n                },\r\n            }).then((resposta) =>  {\r\n            return resposta\r\n        })\r\n    }\r\n  };","C:\\Users\\marcelo.nogueira\\OneDrive - A. Telecom Teleinformática LTDA\\Documents\\aponta-client\\src\\componentes\\TempoReal\\TempoReal.tsx",[],[],"C:\\Users\\marcelo.nogueira\\OneDrive - A. Telecom Teleinformática LTDA\\Documents\\aponta-client\\src\\stores\\actions\\LoginAction.ts",["133","134","135"],"export function SetUsuarioLogado(usuarioLogado : any) {\r\n    return {\r\n      type: \"SET_USUARIO_LOGADO\",\r\n      usuarioLogado,\r\n    };\r\n  }","C:\\Users\\marcelo.nogueira\\OneDrive - A. Telecom Teleinformática LTDA\\Documents\\aponta-client\\src\\stores\\reducers\\indexReducer.ts",[],"C:\\Users\\marcelo.nogueira\\OneDrive - A. Telecom Teleinformática LTDA\\Documents\\aponta-client\\src\\store